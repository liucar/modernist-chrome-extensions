{
  "manifest_version": 2,

  "name": "ROTATE",
  "description": "Turns images, body, divs, text.",
  "version": "1.0",
  "browser_action": { },

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },


    "background": {
    "scripts": ["background.js"]
  },

  
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }
  ],

  "permissions": [
   "activeTab"
   ]
}



